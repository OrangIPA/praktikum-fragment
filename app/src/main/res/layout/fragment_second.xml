<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/the_numbers" />

    <TextView
        android:id="@+id/nama2"
        android:layout_width="8dp"
        android:layout_height="29dp"
        android:layout_marginTop="272dp"
        android:text=":"
        android:textSize="22sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/namaLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="Nama"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/nama2"
        app:layout_constraintTop_toTopOf="@+id/nama2" />

    <TextView
        android:id="@+id/namaRight"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text=""
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/nama2"
        app:layout_constraintTop_toTopOf="@+id/nama2" />

    <TextView
        android:id="@+id/noTelp2"
        android:layout_width="8dp"
        android:layout_height="29dp"
        android:layout_marginTop="20dp"
        android:text=":"
        android:textSize="22sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namaRight" />

    <TextView
        android:id="@+id/alamat2"
        android:layout_width="8dp"
        android:layout_height="29dp"
        android:layout_marginTop="20dp"
        android:text=":"
        android:textSize="22sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noTelpRight" />

    <TextView
        android:id="@+id/noTelpLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="No Telepon"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/noTelp2"
        app:layout_constraintTop_toTopOf="@+id/noTelp2" />

    <TextView
        android:id="@+id/alamatLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="Alamat"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/noTelp2"
        app:layout_constraintTop_toTopOf="@+id/alamat2" />

    <TextView
        android:id="@+id/noTelpRight"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text=""
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/noTelp2"
        app:layout_constraintTop_toTopOf="@+id/noTelp2" />

    <TextView
        android:id="@+id/alamatRight"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/noTelpRight"
        app:layout_constraintTop_toTopOf="@+id/alamat2" />
</androidx.constraintlayout.widget.ConstraintLayout>